    <record name="CdrColor" size="4">
      <field start="0" end="0" type="quint8" name="d1" />
      <field start="1" end="1" type="quint8" name="d2" />
      <field start="2" end="2" type="quint8" name="d3" />
      <field start="3" end="3" type="quint8" name="d4" />
    </record>

    <record name="CdrArgumentListData" size="6">
<!--    // TODO: convert endianness on bigendian system // void convertToBugEndian(); -->
      <method>
<code><![CDATA[const quint16* argOffsets() const
{ return reinterpret_cast<const quint16*>( reinterpret_cast<const char*>(this)+__startOfArgs ); }]]></code>
      </method>
      <method>
<code><![CDATA[template<typename T>
T arg( int i ) const
{ return *reinterpret_cast<const T*>( reinterpret_cast<const char*>(this)+argOffsets()[i] ); }]]></code>
      </method>
      <method>
<code><![CDATA[template<typename T>
const T* argPtr( int i ) const
{ return reinterpret_cast<const T*>( reinterpret_cast<const char*>(this)+argOffsets()[i] ); }]]></code>
      </method>

      <field start="0" end="1" name="chunkLength" type="quint16">
        <description>size of data</description>
      </field>
      <field start="2" end="3" name="count" type="quint16">
        <description>number of arguments</description>
      </field>
      <field start="4" end="5" name="startOfArgs" type="quint16">
        <description>start of argument pointers, relativ to begin of this record.</description>
      </field>
    </record>

    <record name="CdrArgumentWithTypeListData" base="CdrArgumentListData" size="8">
      <method>
<code><![CDATA[const quint16* argTypes() const
{ return reinterpret_cast<const quint16*>( reinterpret_cast<const char*>(this)+__startOfArgTypes ); }]]></code>
      </method>
      <method><!--order of types seems inverted...-->
<code><![CDATA[quint16 argType( int i ) const { return argTypes()[count()-i-1]; }]]></code>
      </method>
      <field start="6" end="7" name="startOfArgTypes" type="quint16">
        <description>start of argument types, relativ to begin of this record.</description>
      </field>
    </record>

    <record name="CdrObjectArgumentsData" base="CdrArgumentWithTypeListData" size="10">
      <field start="8" end="9" name="chunkType" type="quint16">
          <description>The type of the object, see CdrObjectIds.</description>
      </field>
    </record>
  </records>

